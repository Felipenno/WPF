        <Label Content="CPF" FontSize="14" HorizontalAlignment="Left" Margin="26,115,0,0" VerticalAlignment="Top"/>
        <TextBox Name="boxCpfCliente" MaxLength="11" HorizontalAlignment="Left" FontSize="14" Height="29" Margin="79,115,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="274"/>
